<h1>Create a New Video</h1>

<%= form_for @video do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  </br>
  <%= f.label :youtube_id %>
  <%= f.text_field :youtube_id %>
  </br>
  <%= f.label :description %>
  <%= f.text_field :description%>
  </br>
  <%= f.submit :'Click to Submit'%>
<% end %>